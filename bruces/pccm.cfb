	if	(1-\L.A)!(1-\L.W)		:  in multi-versions ###sln
L.A	EQ	0020
L.W	EQ	0040
	ei

sy.u	eq	L.A!L.R
sy.l	eq	L.SYA!L.S0A!L.DISC!L.SYP!L.S0R!L.H!L.A!L.P!L.W!L.E!L.HALT
