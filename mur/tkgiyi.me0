PROCEDURE GEN.THRESHOLD
BEGIN

  CONTROLS FOR RELATIONS CONCURRENT UPDATE

REPEAT.LOOP:
BEGIN

  TYPE @CR, "ENTER CLASS ABBREVIATION: "
  ACCEPT ABBREV
  TYPE @CR, "ENTER PERCENTAGE: "
  ACCEPT UNADJSTD.PERCENT
  TYPE @CR, "ENTER CRASHES: "
  ACCEPT UNADJSTD.CRASHES
  LET ADJSTD.PERCENT = 0
  LET ADJSTD.CRASHES = 0
  INSERT INTO THRESHOLD.
  TYPE @CR, "MORE: "
  ACCEPT ABBREV
  IF ABBREV = "YES" THEN REPEAT REPEAT.LOOP

END
END
