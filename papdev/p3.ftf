     6  'WTSSF','UKFR ','ATS  ','CANOP','     ','CTSDE','CTSSL','EIMGT',
     7  'EPSG ','ETSBO','NSSCP','FTMSG','PORTS','MCI','DBVAL','ROTRY',
     8  'UKMSG','BTGER','PI','BTWCA','CFSS','CONFR','BTSWE','ATCOM',
     9  'CLOSE','TELCO','ETSG ','ORDIN','DSOS','NTD','RNMCA',
     A  'VIDSP','BTNL','HTSUK','BTFR','CPSS','STSUK','CS8CU','GTSAR',
     B  'ORDCU','CWSRV','GTSMX','NIS','SIRTI','GTSCO','GTSVE','DLCOP',
     C  'GTSZP','BTJPN','HKSW ','GTSZR','EMSG','BTBEL','ETSRE','DTPSC',
     D  'ETSAT','BTKOR','CSGER','BTAP','BTUK','FSNL ','FSBEL','BTSZ',
     E  'UKCS ','BTITA','BTSP','TSBEL','TSNL ','EUROP','SNGTL','BTAUS',
     F  'NBILL','BTHK','FSFR','UK800','SPNMC','EDICN','MSGCN',
     G  'TIS  ','UKCUS','UKCLR','NTENG','TPVIE','NOVPS','FRCS ','GSTC ',
     F  'FLM2 ','IPLC ','ECSC ','ENMC ','FSGER','FSITA','FSSZ ',
     I  'FSSPA','FSSWE','BTSNG','ITSNY','GCSCL','FLM1 ','EDITS',
     J  'HSMC ','NTNOR','SDSKO','PHCOM','PADSP','SDSMA','MAYTL'/
        DATA (ATTN(I),I=150,170)/'BTALS','OTHER','TRAIN','ERIM',
     1  'DOHP ','NSCEA','LIVER','NCMON','GCSCA','GCSC1','ERNMC',
     2  '     ','     ','     ','     ','     ','     ','     ',
     3  '     ','     ','     '/

        NUMATTN=160
        
        CALL DBERR($10,IERT,IERC,0)
        GOTO 15
10      IF (IERT.NE.7) GOTO 999
        TYPE 13
13      FORMAT(1X,'DATA BASE BUSY... WAITING 10 SECS.')
        CALL WAIT(5.0)
15      CALL DBOPEN('(TNT)PROFIL')
        CALL DBUPD(-1)
        CALL DBFIND(17,1,WELL,1,18,1,IPXNA,1,6,1,CRDNM,1,5,1,PRTNM)
        CALL DBNREC(RECORD)
        IF (RECORD.GT.0) GOTO 50
        Z=1
        CALL DBFIND(17,1,'Z')
        CALL DBGREC($999,1)
        GOTO 55
50      CALL DBGREC($999,1)
55      CALL DBVAL(1,SITC,2,SITP,3,NET,4,NOD1,5,SLOT,6,HST1,7,CIRCKT,
     1  8,TROUB1,9,TECH,10,CNAM,11,KHST,12,COV,13,PRO,14,STH,15,EHR,16,
     2  PORT,18,PROD,21,LOC,22,VER,23,T40,25,SRGN,26,HRGN,27,LIN1,
     3  28,SNAM,29,SDAT,30,INT,31,F1)
        CALL DBNDAT(SDAT,IM,ID,IY)
57      FORMAT(2A5)
        SLOT=PRTNM
        HST1=CRDNM
        PROD=IPXNA

        TYPE 60
60      FORMAT(' BEGIN CHECK FOR EMPTY FIELDS',/)
        CALL GTHR3('CUSTOMER NAME:  ',CNAM,Z,2)
        IF (LOC(1).EQ.'     ') CALL GTWO('SITE LOCATION:  ',LOC,Z,2)
        IF (SITC(1).EQ.'     ') CALL GTHRE('SITE CONTACT:  ',SITC,Z)
        IF (SITP(1).EQ.'     ') CALL GTHRE('SITE PHONE #:  ',SITP,Z)
        IF (NOD1.EQ.0) CALL GTW3('FR NODE #:  ',NOD1,Z)
        IF (CIRCKT(1).EQ.'     ') CALL GTWO('CIRCUIT #:  ',CIRCKT,Z,2)
        IF (TROUB1(1).EQ.'     ') CALL GTHRE('IP ADDRESS:  ',TROUB1,Z)
        IF (KHST.EQ.0) CALL GONE('DLCI:  ',KHST,Z)
        IF (PRO.EQ.0) CALL BNDLD(PRO,Z)
        IF(TECH(1).EQ.'     ') CALL GTHR3('COMM SPECIALIST:  ',
     1  TECH,Z,1)
        IF (SRGN.EQ.'     ') CALL GRGN(1,P,Z,NUMATTN)
        IF ((SRGN.EQ.'     ').AND.(P.GT.0))DECODE(5,65,ATTN(P))SRGN
        IF (HRGN.EQ.'     ') CALL GRGN(2,P,Z,NUMATTN)
        IF ((HRGN.EQ.'     ').AND.(P.GT.0))DECODE(5,65,ATTN(P))HRGN
65      FORMAT(A5)
        IF (F1.EQ.0) CALL GZONE(F1,Z)


        RETURN
999     TYPE 1000,IERT,IERC
1000    FORMAT(/1X,'ERROR TYPE = ',I3,4X,'ERROR CODE = ',I3,/)
        IF(CCC.EQ.0) CCC=1
        RETURN
        END
