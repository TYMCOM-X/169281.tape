C
C       ROUTINE TO CHANGE A TIME FIELD
C
5650    TYPE 5655
5655    FORMAT(/1X,'ROUTINE TO CHANGE A TIME FIELD',/2X,'1 = ',
     1  'CHANGE BASE1',/2X,'2 = CHANGE BASE4',/2X,'3 = CHANGE BASE7',
     2  /1X,'ENTER SELECTION : ',$)
        ACCEPT 120,K
        IF ((K.LE.0).OR.(K.GT.3)) GOTO 5650
        IF (K.EQ.1) GOTO 5680
        IF (K.EQ.2) GOTO 5700
        CALL DBOPEN('(TNT)BASE7')
        CALL DBUPD(-1)
        CALL DBFIND('ELM',1,0)
        CALL DBNREC(NUMBER)
        IF (NUMBER.EQ.0) GOTO 59
        DO 5679 I=1,NUMBER
        CALL DBGREC($5650,I)
        CALL DBVAL(1,TNUM,'TIME',TIM1,'NTIM',TUP,'ATIM',TCLSD)
        CALL CHFLD(TNUM,I,TIM1,TUP,TCLSD,Z)
        IF (Z.GT.0) CALL DBCHNG('TIME',TIM1,'NTIM',TUP,'ATIM',TCLSD)
5679    CONTINUE
        CALL DBCLOSE
        GOTO 4950
5680    CONTINUE
5700    CONTINUE
