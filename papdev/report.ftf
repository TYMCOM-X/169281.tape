5610    FORMAT('+',I5)
        CALL DBGREC($5634,I)
        CALL DBVAL(1,TNUM,5,SDAT,6,TIM1,20,SRGN,21,HRGN,35,V6,
     1  52,SVER,53,HN,54,ELM,47,F1,49,DUP,50,TUP,55,COC)
        DECODE(5,5615,TIM1)V1,V2
5615    FORMAT(I2,1X,I2)
        DECODE(5,5615,TUP)V3,V4
        CALL DBSET(2)
        CALL DBADD(1,TNUM,2,SDAT,3,V1,4,V2,5,V3,6,V4,7,SVER,8,DUP,
     1  9,COC,10,ELM,11,HRGN,12,SRGN,13,F1,14,HN,15,V6)
        CALL DBSET(1)
5630    CONTINUE
        TOGO=TOGO+100
        DONE=DONE+100
