        CALL GETC(I,IN,PCE)
        IF ((PCE.GE."141).AND.(PCE.LE."172)) PCE=PCE-"040
        CALL PUTC(I,IN,PCE)
150     CONTINUE
        RETURN
        END
        SUBROUTINE UPCAS3(VALUE)
