        CALL DBUPD(-1)
        CALL DBFIND(17,1,WELL,1,18,1,IPXNA,1,6,1,CRDNM,1,5,1,PRTNM)
        CALL DBNREC(RECORD)
        IF (RECORD.GT.0) GOTO 50
