c       routine to force characters to upper case
c
c****************************************
c
        SUBROUTINE UPCASE(VALUE)
c
        integer value(4),pce,i,j,k
c
100     do 150 i=1,20
        call getc(i,value,pce)
        if ((pce.ge."141).and.(pce.le."172)) pce=pce-"040
        call putc(i,value,pce)
150     continue
        return
        end
